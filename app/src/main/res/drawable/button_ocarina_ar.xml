<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape>
            <gradient android:angle="270"
                android:endColor="@color/white_dark"
                android:startColor="@color/white"/>
